{
  "name": "top20-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "cd ./node_modules//@toplast/lastfm && yarn install && yarn run build",
    "build": "next build",
    "clean": "eslint --fix ./src ./cypress; prettier --write \"./(src|cypress)/**/*.+(ts|tsx|js|jsx|json|yml|yaml|css|scss|md|vue)\"",
    "compile": "tsc",
    "coverage": "jest test --coverage --coverageDirectory=coverage --watchAll=false",
    "dev": "next dev",
    "lint": "eslint ./src ./cypress",
    "newkey": "jose newkey -s 256 -t oct -a HS512",
    "preinstall": "npx npm-force-resolutions",
    "security": "audit-ci --config .audit-ci.json",
    "smoke:dev": "cypress open",
    "smoke:env": "node cypress/utils/env.mjs",
    "smoke:run": "cypress run --config video=false,screenshotOnRunFailure=false",
    "start": "next start",
    "test": "jest --watch"
  },
  "resolutions": {
    "axios": ">=0.21.2",
    "json-schema": " >=0.4.0",
    "node-fetch": "^2.6.7"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.56.0",
    "@aws-sdk/signature-v4-crt": "^3.56.0",
    "@chakra-ui/clickable": "^1.2.4",
    "@chakra-ui/icons": "^1.1.5",
    "@chakra-ui/react": "^1.8.6",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.6.0",
    "@toplast/lastfm": "https://github.com/niall-byrne/lastfm#dev",
    "cheerio": "^1.0.0-rc.10",
    "ci": "^2.1.1",
    "d3": "^7.4.4",
    "express-validator": "^6.14.0",
    "flagsmith": "^3.4.1",
    "flagsmith-nodejs": "^2.3.0",
    "formik": "^2.2.9",
    "framer-motion": "^5.6.0",
    "js-cookie": "^3.0.1",
    "next": "^12.3.0",
    "next-auth": "^4.10.3",
    "next-connect": "^0.11.1",
    "next-i18next": "^8.10.0",
    "node-fetch": "^2.6.7",
    "react": "^17.0.2",
    "react-card-flip": "^1.1.5",
    "react-cookie-consent": "^7.3.1",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-ga": "^3.3.0",
    "react-icons": "^4.3.1",
    "react-social-login-buttons": "^3.6.0",
    "use-persisted-reducer": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@cypress/react": "^5.12.4",
    "@cypress/webpack-dev-server": "^1.8.3",
    "@emotion/jest": "^11.7.1",
    "@netlify/plugin-nextjs": "^4.16.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/d3": "^7.1.0",
    "@types/jest": "^27.4.1",
    "@types/js-cookie": "^3.0.1",
    "@types/react": "^17.0.43",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "audit-ci": "^5.1.2",
    "babel-jest": "^27.5.1",
    "cypress": "^9.5.2",
    "deep-keys": "^0.5.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.1.1",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-unused-imports": "^1.1.5",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-specific-snapshot": "^5.0.0",
    "next-router-mock": "^0.6.5",
    "node-jose-tools": "^1.6.28",
    "node-mocks-http": "^1.11.0",
    "npm-force-resolutions": "^0.0.10",
    "prettier": "^2.6.1",
    "typescript": "^4.6.3",
    "webpack": "^5.70.0",
    "webpack-dev-server": "^4.7.4"
  },
  "engines": {
    "npm": "^6.0.0",
    "node": "^14.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/src/**/fixtures/*.{js,jsx,ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/pages/_app.tsx",
      "<rootDir>/src/pages/_document.tsx",
      "<rootDir>/src/pages/api/auth"
    ],
    "coverageReporters": [
      "text",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    },
    "globalSetup": "<rootDir>/jest/globalSetup.js",
    "setupFilesAfterEnv": [
      "<rootDir>/jest/setup/emotion.js",
      "<rootDir>/jest/setup/fetch.js",
      "<rootDir>/jest/setup/i18next.js",
      "<rootDir>/jest/setup/next.jsx",
      "<rootDir>/jest/setup/snapshot.js",
      "<rootDir>/jest/setup/testing-library.js"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(png|jpg|ttf|woff|woff2)$": "<rootDir>/jest/modules/images.js",
      "d3": "<rootDir>/node_modules/d3/dist/d3.min.js"
    },
    "roots": [
      "./src"
    ],
    "testEnvironment": "jsdom",
    "verbose": true
  }
}
