{
  "name": "top20-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "clean": "eslint --fix ./src ./cypress; prettier --write \"./(src|cypress)/**/*.+(ts|tsx|js|jsx|json|yml|yaml|css|scss|md|vue)\"",
    "compile": "tsc",
    "coverage": "jest test --coverage --coverageDirectory=coverage --watchAll=false",
    "dev": "next dev",
    "lint": "eslint ./src ./cypress",
    "newkey": "jose newkey -s 256 -t oct -a HS512",
    "preinstall": "npx npm-force-resolutions",
    "security": "audit-ci --config .audit-ci.json",
    "smoke:dev": "cypress open",
    "smoke:env": "node cypress/utils/env.mjs",
    "smoke:run": "cypress run --config video=false,screenshotOnRunFailure=false",
    "start": "next start",
    "test": "jest --watch"
  },
  "resolutions": {
    "axios": ">=0.21.2",
    "json-schema": " >=0.4.0",
    "node-fetch": "^2.6.7"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.48.0",
    "@aws-sdk/signature-v4-crt": "^3.47.2",
    "@chakra-ui/clickable": "^1.2.1",
    "@chakra-ui/icons": "^1.1.1",
    "@chakra-ui/react": "^1.8.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@toplast/lastfm": "^0.1.5",
    "cheerio": "^1.0.0-rc.10",
    "ci": "^2.1.1",
    "express-validator": "^6.14.0",
    "formik": "^2.2.9",
    "framer-motion": "^5.6.0",
    "js-cookie": "^3.0.1",
    "next": "^11.1.3",
    "next-auth": "^4.1.2",
    "next-connect": "^0.11.0",
    "next-i18next": "^8.10.0",
    "node-fetch": "^2.6.7",
    "react": "^17.0.2",
    "react-card-flip": "^1.1.5",
    "react-cookie-consent": "^7.2.1",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-ga": "^3.3.0",
    "react-icons": "^4.3.1",
    "react-social-login-buttons": "^3.6.0",
    "use-persisted-reducer": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@cypress/react": "^5.12.1",
    "@cypress/webpack-dev-server": "^1.8.0",
    "@emotion/jest": "^11.7.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.4.0",
    "@types/js-cookie": "^3.0.1",
    "@types/next": "^9.0.0",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "audit-ci": "^5.1.2",
    "babel-jest": "^27.4.6",
    "cypress": "^9.3.1",
    "deep-keys": "^0.5.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.8",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "next-router-mock": "^0.6.5",
    "node-jose-tools": "^1.6.28",
    "node-mocks-http": "^1.11.0",
    "npm-force-resolutions": "^0.0.10",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-dev-server": "^4.7.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/src/**/fixtures/*.{js,jsx,ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/pages/_app.tsx",
      "<rootDir>/src/pages/_document.tsx",
      "<rootDir>/src/pages/api/auth"
    ],
    "coverageReporters": [
      "text",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    },
    "globalSetup": "<rootDir>/jest/globalSetup.js",
    "setupFilesAfterEnv": [
      "<rootDir>/jest/setup/emotion.js",
      "<rootDir>/jest/setup/fetch.js",
      "<rootDir>/jest/setup/i18next.js",
      "<rootDir>/jest/setup/testing-library.js"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(png|jpg|ttf|woff|woff2)$": "<rootDir>/jest/modules/images.js"
    },
    "roots": [
      "./src"
    ],
    "testEnvironment": "jsdom",
    "verbose": true
  }
}
