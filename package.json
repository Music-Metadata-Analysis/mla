{
  "name": "top20-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "preinstall": "npx npm-force-resolutions",
    "build": "next build",
    "start": "next start",
    "lint": "eslint ./src/",
    "clean": "eslint --fix ./src; prettier --write \"./src/**/*.+(ts|tsx|js|jsx|json|yml|yaml|css|scss|md|vue)\"",
    "coverage": "jest test --coverage --coverageDirectory=coverage --watchAll=false",
    "test": "jest --watch"
  },
  "dependencies": {
    "@toplast/lastfm": "^0.1.3",
    "express-validator": "^6.12.0",
    "next": "11.0.1",
    "next-connect": "^0.10.1",
    "node-fetch": "^2.6.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-ga": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/jest": "^26.0.23",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "17.0.11",
    "babel-jest": "^27.0.5",
    "eslint": "^7.29.0",
    "eslint-config-next": "11.0.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-unused-imports": "^1.1.2",
    "jest": "^27.0.5",
    "node-mocks-http": "^1.10.1",
    "npm-force-resolutions": "0.0.10",
    "prettier": "^2.3.2",
    "typescript": "4.3.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "src/serviceWorker.js",
      "src/service-worker.js"
    ],
    "coverageReporters": [
      "text",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    },
    "globalSetup": "<rootDir>/jestSetup.js",
    "testEnvironment": "jsdom",
    "verbose": true
  }
}
